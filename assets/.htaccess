## handle cache expiration times
ExpiresActive On
ExpiresByType text/html A600
ExpiresByType text/javascript A600

<ifModule mod_headers.c>
<FilesMatch "PACKAGES$|PACKAGES\.gz$|VIEWS">
Header set Cache-Control "max-age=3600"
</FilesMatch>
<FilesMatch "\.json$|\.js$">
Header set Cache-Control "max-age=600"
</FilesMatch>
<FilesMatch "\.rss$">
Header set Cache-Control "max-age=300"
</FilesMatch>
<FilesMatch "^biocLite\.R$">
Header set Cache-Control "max-age=60"
</FilesMatch>
<FilesMatch "\.tar.gz$|\.tgz$|\.zip$">
Header set Cache-Control "max-age=15552000"
</FilesMatch>
<FilesMatch "\.svg$">
Header set Cache-Control "max-age=30"
</FilesMatch>
<FilesMatch "\.csv$">
Header set Cache-Control "max-age=30"
</FilesMatch>

</ifModule>

<IfModule mod_rewrite.c>
RewriteEngine On
DirectorySlash Off
RewriteBase /

# Redirect www. to non-www
RewriteCond %{HTTP_HOST} ^www\.(.*)$ [NC]
RewriteRule ^(.*)$ http://%1/$1 [R=301,L]

RewriteRule ^(.*)/index_html$ $1/ [R=301]

RewriteRule ^developers/how-to/git-mirror/$ /developers/how-to/git-mirrors/ [PT]

RewriteRule ^BioC2016/$ /help/course-materials/2016/BioC2016/ [R=301]




# handle omitted trailing slash:
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} -d
RewriteCond %{REQUEST_URI} !(.*)/$
RewriteRule ^(.*)$ $1/ [PT]


